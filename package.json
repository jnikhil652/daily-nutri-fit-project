    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "babel-preset-expo": "^13.2.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "react-test-renderer": "^19.1.1",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3"
